<h3>Edit Application Page</h3>

<%= form_for @application do |f| %>
  <%= f.label :company %>
  <%= f.text_field :company %><br/>
  <%= f.label :position %>
  <%= f.text_field :position %><br/>
  <%= f.label :description %>
  <%= f.text_area :description %><br/>
  <%= f.label :notes %>
  <%= f.text_area :notes %><br/>
  <%= f.submit %>
<% end %>
